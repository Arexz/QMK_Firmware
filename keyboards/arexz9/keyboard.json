{
    "manufacturer": "Arexz",
    "keyboard_name": "Arexz9",
    "maintainer": "Arexz",
    "bootloader": "rp2040",
    "processor": "RP2040",
    "firmware_format": "uf2",
    "diode_direction": "ROW2COL",

    "url": "https://github.com/Arexz/qmk_firmware/tree/master/keyboards/arexz9",

    "features": {
        "bootmagic": true,
        "backlight": false,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true

    },

    "matrix_pins": {
        "cols": ["GP4", "GP2", "GP1"],
        "rows": ["GP27", "GP6", "GP26"],
    },

    "usb": {
        "device_version": "1.0.1",
        "pid": "0x1310",
        "vid": "0xF055",
        "force_nkro": true
    },

    "layouts": {
        "LAYOUT_ortho_3x3": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 },
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [1, 0], "x": 0, "y": 1 },
                { "matrix": [1, 1], "x": 1, "y": 1 },
                { "matrix": [1, 2], "x": 2, "y": 1 },
                { "matrix": [2, 0], "x": 0, "y": 2 },
                { "matrix": [2, 1], "x": 1, "y": 2 },
                { "matrix": [2, 2], "x": 2, "y": 2 }
            ]
        }
    }
}
